r }} <sup>{{ pow }}</sup> = {{ squareCounter }}\n  </p>\n\n  <div>\n    <button @click=\"decrease()\">-1</button>\n    <button @click=\"increase()\">+1</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    title: String,\n    start: {\n      type: Number,\n      required: true,\n      default: 0,\n    },\n  },\n\n  data() {\n    return {\n      counter: this.start,\n      pow: 3,\n    };\n  },\n\n  methods: {\n    increase() {\n      this.counter += 1;\n    },\n\n    decrease() {\n      this.counter -= 1;\n    },\n  },\n\n  computed: {\n    squareCounter() {\n      return Math.pow(this.counter, this.pow);\n    },\n    customTitle() {\n      return this.title ? this.title : \"Counter\";\n    },\n  },\n};\n</script>\n\n<style>\nbutton {\n  background-color: black;\n  border-radius: 5px;\n  color: aliceblue;\n  margin: 0 5px;\n  cursor: pointer;\n  padding: 5px 15px;\n  transition: 0.3s ease-in-out;\n}\n\nbutton:hover {\n  background-color: rgba(0, 0, 0, 0.466);\n  transition: 0.3s ease-in-out;\n}\n</style>\n"],"sourceRoot":""}]}